    #set($individuals = $_XPathTool.selectNodes($contact.getChild("individual"), "contact[name !='']") )
    ##physical location:
    #set($address = $contact.getChild('address'))
    #set($building = $_EscapeTool.xml($address.getChild('building').value))
    #set($line = $_EscapeTool.xml($address.getChild('line').value))
    #set($line2 = $_EscapeTool.xml($address.getChild('line2').value))
    #set($city = $_EscapeTool.xml($address.getChild('city').value))
    #set($state = $_EscapeTool.xml($address.getChild('state').value))
    #set($country = $_EscapeTool.xml($address.getChild('country').value))
    #set($zip = $_EscapeTool.xml($address.getChild('zip').value))
    #set($map = $_EscapeTool.xml($address.getChild('map').value))
    #set($hours = $_SerializerTool.serialize($contact.getChild('hours'), true))
    ##mailing address: 
    #if($contact.getChild("mailingAddrType").value == "Rinker campus in Irvine")         
        #set($mLine = "Chapman University")
        #set($mLine2 = "9401 Jeronimo Road")
        #set($mCity = "Irvine")
        #set($mState = "CA")
        #set($mCountry = "")
        #set($mZip = "92618")
    #elseif($contact.getChild("mailingAddrType").value == "Custom address")
        #set($mailingAddress = $contact.getChild('mAddress'))
        #set($mLine = $_EscapeTool.xml($mailingAddress.getChild('line').value))
        #set($mLine2 = $_EscapeTool.xml($mailingAddress.getChild('line2').value))
        #set($mCity = $_EscapeTool.xml($mailingAddress.getChild('city').value))
        #set($mState = $_EscapeTool.xml($mailingAddress.getChild('state').value))
        #set($mCountry = $_EscapeTool.xml($mailingAddress.getChild('country').value))
        #set($mZip = $_EscapeTool.xml($mailingAddress.getChild('zip').value))
    #else
        #set($mLine = "Chapman University")
        #set($mLine2 = "One University Drive")
        #set($mCity = "Orange")
        #set($mState = "CA")
        #set($mCountry = "")
        #set($mZip = "92866")
    #end    
     
    <div class="callout-box-widget--left callout callout-box-widget--contact-widget">
        <div class="border"> </div>     
	    <h2>Contact Us</h2>
	    
        <div class="contact-widget">
            <div class="editableContent">
                #if ($dept != '')
                    <p>${dept}</p>
                #end
                <p>
                    #if ($phone != '')
                        <a href="tel:${sPhone}">${phone}</a>
                        <br/>
                    #end
                    #if ($email != '')
                        <a href="mailto:${email}">${email}</a>
                    #end
                </p>
                
            #if ($suppContacts.size() >= 1)
                <div class="contact">
                #foreach($suppContact in $suppContacts)
                    #set($suppContactLabel = $_XPathTool.selectSingleNode($suppContact, "label").value)
                    #set($suppContactPhone = $_XPathTool.selectSingleNode($suppContact, "phone").value)
                    #set($suppContactPhoneStripped = $suppContactPhone.replace("(", "").replace(")", "").replace("-", "").replace(" ", ""))
                    #set($suppContactEmail = $_XPathTool.selectSingleNode($suppContact, "email").value)
                    <p>
                        #if ($suppContactLabel != "")
                            $suppContactLabel<br/>
                        #end
                        #if ($suppContactPhone != "")
                            <a href="tel:${suppContactPhoneStripped}">${suppContactPhone}</a><br/>
                        #end
                        #if ($suppContactEmail != "")
                            <a href="mailto:$suppContactEmail">$suppContactEmail</a><br/>
                        #end
                    </p>
                #end
                </div>
            #end
            
            ###if (!$_PropertyTool.isNull($contact.getChild('hours')))
            #if ($contact.getChild('hours').value != "" or $contact.getChild('hours').getChildren().size() > 0)
                <h3>Hours</h3>
                ## hours is a wysiwyg so may already have P tags surrounding it (but may not also) so use div here:
                <div>$hours</div>
            #end
            
            #if ($building != "" or $address.getChild('map').value != "" or $line2 != "")
                <h3>Location</h3>
                <p>
                    #if($building != "")
                        ${building} <br/>
                    #end
                    ${line}
                    #if($line2 != "")
                        <br/>${line2} 
                    #end
                    <br/>
                    #if($city != "")
                        ${city}, 
                    #end
                    ${state} ${zip}
                    #if($country != "")
                        <br/>$_EscapeTool.xml($country)
                    #end
                    #if($address.getChild('map').value != "")
                        #set($map = $_EscapeTool.xml($address.getChild('map').value))
                       <br/><a href="${map}">View Map</a>
                    #end
                </p>
            #end
            
            <div class="more-content inactive">
            
                <h3>Mailing Address</h3>
                <p>
                    ${dept}
                    <br/>${mLine}
                    #if($mLine2 != "")
                        <br/>${mLine2} 
                    #end
                    <br/>${mCity}, ${mState} $_EscapeTool.xml($mZip)
                    #if($mCountry != "")
                        <br/>$_EscapeTool.xml($mCountry)
                    #end
                </p>
                
                #if($individuals && $individuals.size() >= 1)
                    #if ($individuals.size() > 1)
                        <h3>Individuals to Contact</h3>
                    #else
                        <h3>Additional Contact</h3>
                    #end
                     
                    #foreach($ind in $individuals)
                        #set($photo = $_XPathTool.selectSingleNode($ind, "photo/link").value)
                        #set($iName = $_EscapeTool.xml($ind.getChild("name").value))
                        #set($iTitle = $_EscapeTool.xml($ind.getChild("title").value))
                        #set($iDesc = $_SerializerTool.serialize($ind.getChild("desc").value, true))
                        #set($iPhone = $_EscapeTool.xml($ind.getChild("phone").value))
                        #set($iPhoneS = $iPhone.replace("(", "").replace(")", "").replace("-", "").replace(" ", ""))
                        #set($iEmail = $_EscapeTool.xml($ind.getChild("email").value))                        
                        #set($iLinkURL = "")
                        #set ($iLinkType = $ind.getChild('link').getChild('linkType').value )
                        #if ($iLinkType == 'Internal Link')
                        #set ($iLinkURL = $ind.getChild('link').getChild('internalLink').getChild('path').value )
                        #elseif ($iLinkType == 'External Link')
                        #set ($iLinkURL = $_EscapeTool.xml($ind.getChild('link').getChild('externalLink').value))
                        #else 
                        #set ($iLinkURL = $ind.getChild('link').getChild('fileLink').getChild('path').value )
                        #end
                        #set ($iLabel = $_EscapeTool.xml($ind.getChild('link').getChild('label').value))                             
                        <p>
                            ${iName}
                            <br/>${iTitle}
                            #if ($ind.getChild("phone").value != "")
                                <br/><a href="tel:${iPhoneS}">${iPhone}</a> 
                            #end
                            #if ($ind.getChild("email").value != "")
                                <br/><a href="mailto:${iEmail}">${iEmail}</a>
                            #end
                            #if ($iLinkURL != '' && $iLinkURL != '/' && $iLabel != '')
                                <ul class="buttonLinks">
                                    <li><a href="${iLinkURL}" target="_parent">
                                        #if($iLabel != "")
                                            ${iLabel}:
                                        #else
                                            More Information
                                        #end
                                    </a></li>
                                 </ul>
                            #end
                        </p>
                    #end
                #end        
            </div> 
            ## end more-content
            
            ## BEGIN OF Social FollowUs
            #set ( $currentPage = $_XPathTool.selectSingleNode($contentRoot, "//system-page[@current]") )
            #set ( $data        = $currentPage.getChild('system-data-structure') )
            ## Get the data for the social accounts
            ##
            ## The modular templates social accounts use data defs at  //system-data-structure/meta/social_accounts
            ## The standard templates social accounts use data defs at //system-data-structure/social_accounts
            ##
            ## this format used for both old templates and new Modular templates. 
            ## New modular templates have social_accounts under a new parent node called meta so check for that before proceeding so get right node
            ##
            #if ($data.getChild('meta').getChild('social_accounts')) 
                #set ( $social_node = $data.getChild('meta').getChild('social_accounts') )    
            #else 
                #set ( $social_node = $data.getChild('social_accounts') )
            #end
            ##
            #set ( $accounts = $social_node.getChild('content').getChild('system-data-structure').getChildren('account') ) 
            #set ( $length = $accounts.size() )
            #set ( $socialAccounts = $social_node.getChild('path').value )
            ##
            ## Output the html for the accounts
            ##
            #if ( $accounts )
            <div class="social-follow-us">
                <h3>Follow Us</h3>
                    <ul class="left-social-icons">
                    #foreach ($account in $accounts)
                        #set ( $name = $account.getChild('account_type').text )
                        #set ( $url = $_SerializerTool.serialize( $account.getChild('account_url'), true) )
                        ## facebook
                        ## twitter
                        ## instagram
                        ## linkedin
                        ## youtube
                        ## pinterest
                        ## vimeo
                        ## snapchat      
                        ## blog
                        ## flickr 
                        ####
                        #if ( $velocityCount == 1 and $length == 1)
                            <li class="last">
                        #elseif ( $velocityCount == 1 )
                            <li>
                        #elseif ( $velocityCount == $length)
                            <li class="last">
                        #else
                            <li>
                        #end
                            <a href="$url" title="$name">
                                <svg style="position: absolute; width: 0; height: 0; overflow: hidden" version="1.1" xmlns="http://www.w3.org/2000/svg" xmlns:xlink="http://www.w3.org/1999/xlink">
                        			<defs>
                                        ##calls macro to define SVG of specific social icon:
                        				#socialSVGicon($name, $url)                    
                        			</defs>
                    			</svg>
                    			<!--#protect<svg class="icon"><use xlink:href="#icon-${name}"></use></svg>#protect-->
                            </a>
                        </li>
                    #end
                </ul>
            </div>
            #end
