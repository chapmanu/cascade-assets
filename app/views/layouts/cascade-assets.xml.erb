<system-xml>    

<![CDATA[#protect
  <!-- <%= "Branch: " + `git rev-parse --abbrev-ref HEAD`.strip + " Build: " + Time.now.strftime('%I:%M%p %m-%d-%Y')  -%> -->
#protect]]> 
 
  <!-- Preload CSS & JS -->
  <%= stylesheet_link_tag 'master', rel: 'preload', as: 'style', media: 'all' %>
  <%= preload_js_link 'master', rel: 'preload', as: 'script', media: 'all'  %>

  <link src="<%=asset_path(Webpacker.manifest.lookup('application.css')&.html_safe) %>" defer="true"/>
  <script src="<%= asset_path(Webpacker.manifest.lookup('application.js')&.html_safe) %>" defer="true"></script>

  <!-- Carry on -->
  <%= stylesheet_link_tag 'master', media: 'all' %>
  <%= javascript_include_tag 'master', defer: true %>

  <link href="//dev-www.chapman.edu/test-section/nick-test/grid-block.css?version=177" id="grid-block-css" rel="stylesheet">
 <script src="https://dev-www.chapman.edu/test-section/nick-test/grid-block.js?version=178"></script>
</system-xml>